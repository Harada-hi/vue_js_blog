{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'TopPage',\n  inject: ['radio', 'blog'],\n  emits: ['postTo'],\n  data() {\n    return {\n      radioTop: [],\n      blogTop: [],\n      loading: true\n    };\n  },\n  methods: {\n    buttonChange() {\n      const radio = document.querySelector('.fa-radio');\n      const blog = document.querySelector('.fa-newspaper');\n      if (radio.classList.contains('no-select') === false) {\n        radio.classList.add('no-select');\n      }\n      if (blog.classList.contains('no-select') === false) {\n        blog.classList.add('no-select');\n      }\n    },\n    postGive() {\n      let ths = this;\n      axios.get('http://pinkhanger.xsrv.jp/wp/wp-json/wp/v2/posts/').then(function (response) {\n        console.log(response);\n        for (let i = 0; i < response.data.length; i++) {\n          if (response.data[i].categories_info[0].name === \"ラジオ\") {\n            let dates = new Date(response.data[i].date);\n            ths.radioTop.push({\n              category: response.data[i].categories_info[0].name,\n              title: response.data[i].title.rendered,\n              date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\n              thumb: response.data[i].thumbnail_url,\n              tag: response.data[i].tags_info[0].name,\n              link: '/post' + response.data[i].id\n            });\n          } else if (response.data[i].categories_info[0].name === \"ブログ\") {\n            let dates = new Date(response.data[i].date);\n            ths.blogTop.push({\n              category: response.data[i].categories_info[0].name,\n              title: response.data[i].title.rendered,\n              date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\n              thumb: response.data[i].thumbnail_url,\n              tag: response.data[i].tags_info[0].name,\n              link: '/post' + response.data[i].id\n            });\n          } else {\n            console.log(\"とばすよ～ん\");\n          }\n        }\n        ths.$emit('postTo', ths.radioTop, ths.blogTop);\n        ths.radioTop = ths.radioTop[0];\n        ths.blogTop = ths.blogTop[0];\n      }).catch(function (error) {\n        console.log(error);\n      });\n    }\n  },\n  created() {\n    this.loading = true;\n  },\n  mounted() {\n    if (this.radio.length === 0) {\n      console.log(\"injectnなし\");\n      this.postGive();\n    } else {\n      console.log(\"injectあり\");\n      console.log(this.blog);\n      this.radioTop = this.radio[0];\n      this.blogTop = this.blog[0];\n      console.log(this.blog.length);\n    }\n    this.buttonChange();\n  },\n  updated() {\n    this.loading = false;\n  }\n};","map":{"version":3,"names":["axios","name","inject","emits","data","radioTop","blogTop","loading","methods","buttonChange","radio","document","querySelector","blog","classList","contains","add","postGive","ths","get","then","response","console","log","i","length","categories_info","dates","Date","date","push","category","title","rendered","getFullYear","getMonth","getDate","thumb","thumbnail_url","tag","tags_info","link","id","$emit","catch","error","created","mounted","updated"],"sources":["C:\\Users\\uhouh\\Documents\\pink-hunger\\pink-hunger\\src\\views\\TopPage.vue"],"sourcesContent":["<template>\r\n  <div v-show=\"loading\" class=\"loader\">\r\n    <div class=\"loader-inner\">\r\n      <p>よみこみちゅうだよ～ん</p>\r\n    </div>\r\n  </div>\r\n  <div v-show=\"!loading\" class=\"top_wrap\">\r\n  \r\n    <div class=\"top_inner\">\r\n      <div class=\"top_img\">\r\n        <img src=\"../utiles/hung.png\">\r\n        <p>たのしいラジオとブログあり。</p>\r\n\r\n      </div>\r\n      <div class=\"content_flex\">\r\n        <div class=\"contents radio\">\r\n          <div class=\"head\">New Radio</div>\r\n          <div class=\"contents_inner\">\r\n            <div class=\"contents_thumb\">\r\n              <router-link v-bind:to=\"radioTop.link\">\r\n              <img :src=\"radioTop.thumb\">\r\n              </router-link>\r\n            </div>\r\n            <div class=\"contents_tag\">\r\n              <p>{{ radioTop.tag }}</p>\r\n            </div>\r\n            <div class=\"contents_title\">\r\n              <router-link v-bind:to=\"radioTop.link\">\r\n              <p>{{ radioTop.title }}</p>\r\n            </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\"contents_link\">\r\n            <router-link to=\"/Radio\">\r\n              <p>ほかの回を聴く</p>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n  \r\n        <div class=\"contents blog\">\r\n          <div class=\"head\">New Blog</div>\r\n          <div class=\"contents_inner\">\r\n            <div class=\"contents_thumb\">\r\n              <router-link v-bind:to=\"blogTop.link\">\r\n              <img :src=\"blogTop.thumb\">\r\n            </router-link>\r\n            </div>\r\n            <div class=\"contents_tag\">\r\n              <p>{{ blogTop.tag }}</p>\r\n            </div>\r\n            <div class=\"contents_title\">\r\n              <router-link v-bind:to=\"blogTop.link\">\r\n              <p>{{ blogTop.title }}</p>\r\n            </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\"contents_link\">\r\n            <router-link to=\"/article\">\r\n              <p>ほかの記事を読む</p>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n  </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios'\r\n  \r\n  \r\n  export default {\r\n    name: 'TopPage',\r\n    inject:['radio','blog'],\r\n    emits:['postTo'],\r\n    data(){\r\n      return {\r\n        radioTop:[],\r\n        blogTop:[],\r\n        loading:true\r\n      }\r\n    },\r\n    methods: {\r\n      buttonChange(){\r\n        const radio = document.querySelector('.fa-radio')\r\n        const blog = document.querySelector('.fa-newspaper')\r\n       \r\n        if(radio.classList.contains('no-select') === false){\r\n          radio.classList.add('no-select')\r\n        }\r\n  \r\n        if(blog.classList.contains('no-select') === false) {\r\n          blog.classList.add('no-select')\r\n        }\r\n      },\r\n      postGive(){\r\n        \r\n        let ths = this;\r\n        axios\r\n      .get('http://pinkhanger.xsrv.jp/wp/wp-json/wp/v2/posts/')\r\n      .then(function(response){\r\n          console.log(response)\r\n        for(let i = 0; i < response.data.length; i++) {\r\n          if(response.data[i].categories_info[0].name === \"ラジオ\"){\r\n            let dates = new Date(response.data[i].date)\r\n            ths.radioTop.push(\r\n              { \r\n                \r\n                category: response.data[i].categories_info[0].name,\r\n                title: response.data[i].title.rendered,\r\n                date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\r\n                thumb: response.data[i].thumbnail_url,\r\n                tag:  response.data[i].tags_info[0].name,\r\n                link : '/post' + response.data[i].id,\r\n              }\r\n              \r\n            )\r\n  \r\n  \r\n        } else if(response.data[i].categories_info[0].name === \"ブログ\"){\r\n          let dates = new Date(response.data[i].date)\r\n          ths.blogTop.push(\r\n            { \r\n              \r\n              category: response.data[i].categories_info[0].name,\r\n              title: response.data[i].title.rendered,\r\n              date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\r\n              thumb: response.data[i].thumbnail_url,\r\n              tag:  response.data[i].tags_info[0].name,\r\n              link : '/post' + response.data[i].id,\r\n            }\r\n            \r\n          )\r\n  \r\n  \r\n      } else {\r\n            console.log(\"とばすよ～ん\")\r\n          }\r\n        }\r\n        ths.$emit('postTo',ths.radioTop,ths.blogTop)\r\n        ths.radioTop = ths.radioTop[0]\r\n        ths.blogTop = ths.blogTop[0]\r\n      })\r\n      .catch(function(error) {\r\n        console.log(error);\r\n      });\r\n      }\r\n    },\r\n    created(){\r\n    this.loading=true\r\n  },\r\n    mounted(){\r\n      if (this.radio.length === 0) {\r\n        console.log(\"injectnなし\")\r\n        this.postGive()\r\n      } else {\r\n        console.log(\"injectあり\")\r\n        console.log(this.blog)\r\n        this.radioTop = this.radio[0]\r\n        this.blogTop = this.blog[0]\r\n        console.log(this.blog.length)\r\n      }\r\n\r\n      this.buttonChange()\r\n    },\r\n    updated(){\r\n      this.loading = false;\r\n    }\r\n    }\r\n  \r\n  </script>\r\n  \r\n  <style>\r\n.loader {\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n  .top_wrap {\r\n    padding: 50px 0 ;\r\n  min-height: calc(100vh);\r\n  background: #f1f0ed;\r\n  }\r\n\r\n  .top_inner {\r\n    max-width: 1200px;\r\n    margin: auto;\r\n    \r\n  }\r\n\r\n  .top_img {\r\n    width: 300px;\r\n    margin: auto;\r\n    \r\n  }\r\n\r\n  .top_img img {\r\n    display: block;\r\n    position: relative;\r\n  }\r\n\r\n  .top_img p {\r\n\r\ntext-align: center;\r\nmargin: 20px 0 40px;\r\nfont-size: 20px;\r\n  }\r\n\r\n\r\n\r\n  .head {\r\n    text-align: center;\r\n    font-weight: bold;\r\n    font-size: 24px;\r\n  }\r\n\r\n  .contents_inner {\r\n    margin: 20px 10px 0;\r\n  }\r\n.contents {\r\n  max-width: 400px;\r\n  margin: auto;\r\n  \r\n\r\n}\r\n\r\n.radio {\r\n  margin-bottom: 20px;\r\n}\r\n \r\n\r\n.contents_thumb {\r\n  max-width: 250px;\r\n  margin: auto;\r\n}\r\n\r\n.contents_title {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n}\r\n\r\n  .contents_tag {\r\n  background: black;\r\n  color: white;\r\n  font-weight: bold;\r\n  font-size: 12px;\r\n  padding: 4px 8px;\r\n  width: 60px; /* 横幅固定 */\r\n  text-align: center; /* 文字中央揃え */\r\n  border-ra