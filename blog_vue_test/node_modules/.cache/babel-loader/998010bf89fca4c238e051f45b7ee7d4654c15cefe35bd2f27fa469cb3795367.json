{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { articles } from '@/utiles/articles';\nexport default {\n  name: 'TopPage',\n  inject: ['tech', 'blog'],\n  emits: ['postTo'],\n  data() {\n    return {\n      techTop: [],\n      blogTop: [],\n      loading: true\n    };\n  },\n  methods: {\n    buttonChange() {\n      const tech = document.querySelector('.fa-radio');\n      const blog = document.querySelector('.fa-newspaper');\n      if (tech.classList.contains('no-select') === false) {\n        tech.classList.add('no-select');\n      }\n      if (blog.classList.contains('no-select') === false) {\n        blog.classList.add('no-select');\n      }\n    },\n    postGet() {\n      let ths = this;\n      for (let i = 0; i < articles.length; i++) {\n        if (articles[i].category === \"技術系\") {\n          ths.techTop.push({\n            category: articles[i].category,\n            title: articles[i].title,\n            date: articles[i].date,\n            thumb: articles[i].thumb,\n            tag: articles[i].tag,\n            link: '/post' + articles[i].id\n          });\n          console.log(ths.techTop);\n        } else if (articles[i].category === \"ブログ\") {\n          ths.blogTop.push({\n            category: articles[i].category,\n            title: articles[i].title,\n            date: articles[i].date,\n            thumb: articles[i].thumb,\n            tag: articles[i].tag,\n            link: '/post' + articles[i].id\n          });\n          console.log(ths.blogTop);\n        } else {\n          console.log(\"skip\");\n        }\n      }\n      ths.$emit('postTo', ths.techTop, ths.blogTop);\n      ths.techTop = ths.techTop[0];\n      ths.blogTop = ths.blogTop[0];\n    }\n  },\n  created() {\n    this.loading = true;\n  },\n  mounted() {\n    if (this.tech.length === 0) {\n      this.postGet();\n    } else {\n      this.techTop = this.tech[0];\n      this.blogTop = this.blog[0];\n    }\n    this.buttonChange();\n  },\n  updated() {\n    this.loading = false;\n  }\n};","map":{"version":3,"names":["articles","name","inject","emits","data","techTop","blogTop","loading","methods","buttonChange","tech","document","querySelector","blog","classList","contains","add","postGet","ths","i","length","category","push","title","date","thumb","tag","link","id","console","log","$emit","created","mounted","updated"],"sources":["C:\\Users\\uhouh\\Documents\\転職用にまとめるための\\pink-hunger\\blog_vue\\src\\views\\TopPage.vue"],"sourcesContent":["<template>\r\n  <div v-show=\"loading\" class=\"loader\">\r\n    <div class=\"loader_inner\">\r\n      <p>読み込み中</p>\r\n    </div>\r\n  </div>\r\n  <div v-show=\"!loading\" class=\"top_wrap\">\r\n  \r\n    <div class=\"top_inner\">\r\n      <div class=\"top_img\">\r\n        <img src=\"../utiles/hung.png\">\r\n        <p>色々やる。</p>\r\n\r\n      </div>\r\n      <div class=\"content_flex\">\r\n        <div class=\"contents tech\">\r\n          <div class=\"head\">New Tech</div>\r\n          <div class=\"contents_inner\">\r\n            \r\n            <div class=\"contents_tag\">\r\n              <p>{{ techTop.tag }}</p>\r\n            </div>\r\n            <div class=\"contents_title\">\r\n              <router-link v-bind:to=\"techTop.link\">\r\n              <p>{{ techTop.title }}</p>\r\n            </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\"contents_link\">\r\n            <router-link to=\"/tech\">\r\n              <p>ほかの記事を読む</p>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n  \r\n        <div class=\"contents blog\">\r\n          <div class=\"head\">New Blog</div>\r\n          <div class=\"contents_inner\">\r\n            \r\n            <div class=\"contents_tag\">\r\n              <p>{{ blogTop.tag }}</p>\r\n            </div>\r\n            <div class=\"contents_title\">\r\n              <router-link v-bind:to=\"blogTop.link\">\r\n              <p>{{ blogTop.title }}</p>\r\n            </router-link>\r\n       