tents_link\">\r\n            <router-link to=\"/blog\">\r\n              <p>ほかの記事を読む</p>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n  </div>\r\n  </template>\r\n  \r\n  <script>\r\n \r\n\r\n  import { articles } from '@/utiles/articles'\r\n\r\n  \r\n  \r\n  export default {\r\n    name: 'TopPage',\r\n    inject:['tech','blog'],\r\n    emits:['postTo'],\r\n    data(){\r\n      return {\r\n        techTop:[],\r\n        blogTop:[],\r\n        loading:true\r\n      }\r\n    },\r\n    methods: {\r\n      buttonChange(){\r\n        const tech = document.querySelector('.fa-radio')\r\n        const blog = document.querySelector('.fa-newspaper')\r\n       \r\n        if(tech.classList.contains('no-select') === false){\r\n          tech.classList.add('no-select')\r\n        }\r\n  \r\n        if(blog.classList.contains('no-select') === false) {\r\n          blog.classList.add('no-select')\r\n        }\r\n      },\r\n      postGet(){\r\n        \r\n        let ths = this;\r\n       for(let i = 0; i < articles.length; i++) {\r\n          if(articles[i].category === \"技術系\"){\r\n            \r\n            ths.techTop.push(\r\n              { \r\n                \r\n                category: articles[i].category,\r\n                title: articles[i].title,\r\n                date: articles[i].date,\r\n                thumb: articles[i].thumb,\r\n                tag:  articles[i].tag,\r\n                link : '/post' + articles[i].id\r\n              }\r\n              \r\n            )\r\n            console.log(ths.techTop)\r\n  \r\n  \r\n        } else if(articles[i].category === \"ブログ\"){\r\n\r\n          ths.blogTop.push(\r\n            { \r\n              \r\n              category: articles[i].category,\r\n                title: articles[i].title,\r\n                date: articles[i].date,\r\n                thumb: articles[i].thumb,\r\n                tag:  articles[i].tag,\r\n                link : '/post' + articles[i].id\r\n            }\r\n            \r\n          )\r\n          console.log(ths.blogTop)\r\n  \r\n  \r\n      } else {\r\n            console.log(\"skip\")\r\n          }\r\n        }\r\n        ths.$emit('postTo',ths.techTop,ths.blogTop)\r\n        ths.techTop = ths.techTop[0]\r\n        ths.blogTop = ths.blogTop[0]\r\n      }\r\n      \r\n      \r\n    },\r\n    created(){\r\n    this.loading=true\r\n  },\r\n    mounted(){\r\n      if (this.tech.length === 0) {\r\n \r\n        this.postGet()\r\n      } else {\r\n\r\n\r\n        this.techTop = this.tech[0]\r\n        this.blogTop = this.blog[0]\r\n\r\n      }\r\n\r\n      this.buttonChange()\r\n    },\r\n    updated(){\r\n      this.loading = false;\r\n    }\r\n    }\r\n  \r\n  </script>\r\n  \r\n  <style>\r\n.loader {\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n  .top_wrap {\r\n    padding: 50px 0 ;\r\n  min-height: calc(100vh);\r\n  background: #ebf0f0;\r\n  }\r\n\r\n  .top_inner {\r\n    max-width: 1200px;\r\n    margin: 20px auto;\r\n    \r\n  }\r\n\r\n  .top_img {\r\n    width: 300px;\r\n    margin: auto;\r\n    \r\n  }\r\n\r\n  .top_img img {\r\n    display: block;\r\n    position: relative;\r\n  }\r\n\r\n  .top_img p {\r\n\r\ntext-align: center;\r\nmargin: 20px 0 40px;\r\nfont-size: 20px;\r\n  }\r\n\r\n\r\n\r\n  .head {\r\n    text-align: center;\r\n    font-weight: bold;\r\n    font-size: 24px;\r\n  }\r\n\r\n  .contents_inner {\r\n    margin: 20px 10px 0;\r\n  }\r\n.contents {\r\n  max-width: 400px;\r\n  padding: 20px 0 15px;\r\n  margin: auto;\r\n  border-radius: 3px;\r\n  box-shadow: 0px 5px 15px 0px rgba(0, 0, 0, 0.35);\r\n    background: rgba(0, 0, 0, 0.05);\r\n  \r\n\r\n}\r\n\r\n.tech {\r\n  margin-bottom: 20px;\r\n}\r\n \r\n\r\n\r\n\r\n.contents_title {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n}\r\n\r\n  .contents_tag {\r\n  background: black;\r\n  color: white;\r\n  font-weight: bold;\r\n  font-size: 12px;\r\n  padding: 4px 8px;\r\n  width: 60px; /* 横幅固定 */\r\n  text-align: center; /* 文字中央揃え */\r\n  border-radius: 5%;\r\n  margin: 10px 0;\r\n  }\r\n\r\n  .contents_link {\r\n    width: 200px;\r\n    margin: 20px auto;\r\n    background: black;\r\n    color: white;\r\n    font-weight: bold;\r\n    font-size: 14px;\r\n    text-align: center;\r\n    border-radius: 10px;\r\n    padding: 10px 0;\r\n  }\r\n  \r\n  .fa-radio {\r\n    color: black;\r\n  }\r\n  \r\n  .fa-newspaper {\r\n    color: black;\r\n  }\r\n  \r\n  .no-select {\r\n    \r\n    color: #818181;\r\n  }\r\n\r\n  @media (min-width: 768px) {\r\n\r\n  .contents {\r\n \r\n    border-radius: 3px;\r\n  box-shadow: 0px 5px 15px 0px rgba(0, 0, 0, 0.35);\r\n    background: rgba(0, 0, 0, 0.05);\r\n  }\r\n.tech {\r\n  margin: auto;\r\n}\r\n\r\n.head {\r\n  margin: 20px 0;\r\n}\r\n\r\n.content_flex {\r\n\r\n  display: flex;\r\n\r\n}\r\n\r\n    .contents_inner {\r\n      margin: 20px 20px 0;\r\n    width: 350px;\r\n\r\n    margin: auto;\r\n    padding: 0 10px;\r\n\r\n    \r\n\r\n  }\r\n\r\n\r\n\r\n  }\r\n  </style>"],"mappings":";OAUaA,UAAwB;;EATbC,KAAK,EAAC;AAAQ;;EAKbA,KAAK,EAAC;AAAU;;EAEhCA,KAAK,EAAC;AAAW;;EAMfA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAe;;EAEnBA,KAAK,EAAC;AAAgB;;EAEpBA,KAAK,EAAC;AAAc;;EAGpBA,KAAK,EAAC;AAAgB;;EAMxBA,KAAK,EAAC;AAAe;;EAOvBA,KAAK,EAAC;AAAe;;EAEnBA,KAAK,EAAC;AAAgB;;EAEpBA,KAAK,EAAC;AAAc;;EAGpBA,KAAK,EAAC;AAAgB;;EAMxBA,KAAK,EAAC;AAAe;;;uBAhDpCC,mBAAA,CAAAC,SAAA,S,gBACEC,mBAAA,CAIM,OAJNC,UAIM,EAAAC,MAAA,QAAAA,MAAA,OAHJF,mBAAA,CAEM;IAFDH,KAAK,EAAC;EAAc,IACvBG,mBAAA,CAAY,WAAT,OAAK,E,wDAFCG,KAAA,CAAAC,OAAO,E,mBAKpBJ,mBAAA,CAmDM,OAnDNK,UAmDM,GAjDJL,mBAAA,CA+CM,OA/CNM,UA+CM,G,0BA9CJN,mBAAA,CAIM;IAJDH,KAAK,EAAC;EAAS,IAClBG,mBAAA,CAA8B;IAAzBO,GAAwB,EAAxBX;EAAwB,IAC7BI,mBAAA,CAAY,WAAT,OAAK,E,sBAGVA,mBAAA,CAwCM,OAxCNQ,UAwCM,GAvCJR,mBAAA,CAkBM,OAlBNS,UAkBM,G,0BAjBJT,mBAAA,CAAgC;IAA3BH,KAAK,EAAC;EAAM,GAAC,UAAQ,sBAC1BG,mBAAA,CAUM,OAVNU,UAUM,GARJV,mBAAA,CAEM,OAFNW,UAEM,GADJX,mBAAA,CAAwB,WAAAY,gBAAA,CAAlBT,KAAA,CAAAU,