{"ast":null,"code":"import { articles } from '@/utiles/articles';\nimport Meta from './meta.vue';\nexport default {\n  name: 'PostComponent',\n  components: {\n    Meta\n  },\n  emits: ['pageTitle'],\n  data() {\n    return {\n      title: '',\n      content: '',\n      date: '',\n      thumb: '',\n      category: '',\n      tag: '',\n      link: '',\n      twitter: '',\n      line: '',\n      blsk: '',\n      facebook: '',\n      loading: true\n    };\n  },\n  created() {\n    this.loading = true;\n  },\n  mounted() {\n    this.postContent();\n  },\n  updated() {\n    this.loading = false;\n  },\n  methods: {\n    buttonChange() {\n      const tech = document.querySelector('.fa-wrench');\n      const blog = document.querySelector('.fa-newspaper');\n      if (this.category === '技術系') {\n        if (tech.classList.contains('no-select')) {\n          tech.classList.remove('no-select');\n          blog.classList.add('no-select');\n        } else {\n          if (blog.classList.contains('no-select') === false) blog.classList.add('no-select');\n        }\n      } else if (this.category === 'ブログ') {\n        if (blog.classList.contains('no-select')) {\n          blog.classList.remove('no-select');\n          tech.classList.add('no-select');\n        } else {\n          if (tech.classList.contains('no-select') === false) tech.classList.add('no-select');\n        }\n      }\n    },\n    postContent() {\n      let ths = this;\n      let forApi = this.$route.path;\n      let idNumber = forApi.replace('/post', '');\n      let id = Number(idNumber) + 1;\n      ths.title = articles[id].title, ths.content = articles[id].content, ths.date = articles[id].date, ths.tag = articles[id].tag, ths.category = articles[id].category, ths.link = 'http://localhost:8080/post' + articles[id].id;\n      ths.twitter = 'https://x.com/intent/tweet?url=' + ths.link + '&text=' + ths.title + ' | ブログ';\n      ths.line = 'https://social-plugins.line.me/lineit/share?url=' + ths.link;\n      ths.blsk = 'https://bsky.app/intent/compose?text=' + ths.title + ' | ブログ' + ths.link;\n      ths.facebook = 'http://www.facebook.com/share.php?u=' + ths.link;\n      ths.buttonChange();\n    }\n  }\n};","map":{"version":3,"names":["articles","Meta","name","components","emits","data","title","content","date","thumb","category","tag","link","twitter","line","blsk","facebook","loading","created","mounted","postContent","updated","methods","buttonChange","tech","document","querySelector","blog","classList","contains","remove","add","ths","forApi","$route","path","idNumber","replace","id","Number"],"sources":["C:\\Users\\uhouh\\Documents\\転職用にまとめるための\\pink-hunger\\blog_vue\\src\\components\\Post.vue"],"sourcesContent":["<template>\r\n<Meta v-bind:pageTitle=\"title\"></Meta>\r\n<div v-show=\"loading\" class=\"loader\">\r\n    <div class=\"loader_inner\">\r\n      <p>読み込み中</p>\r\n    </div>\r\n  </div>\r\n  <div  v-show=\"!loading\" class=\"wrap\">\r\n    <div class=\"wrap-inner\">\r\n      <div class=\"article_inner\">\r\n        <p class=\"title\">{{ title }}</p>\r\n        \r\n        \r\n        \r\n        <div class=\"cat_data\">\r\n          <div class=\"cat_tag\">\r\n          <div class=\"cat\">{{ category }}</div>\r\n          <div class=\"tag\">{{ tag }}</div>\r\n          </div>\r\n          <p>{{ date }}</p>\r\n        \r\n        </div>\r\n        <div class=\"sns-wrap\">\r\n        <div class=\"sns\">\r\n  <a class=\"twitter\" :href=\"twitter\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-x-twitter\"></i>\r\n  </a>\r\n  <a class=\"line\" :href=\"line\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-line\"></i>\r\n  </a>\r\n  <a class=\"bluesky\" :href=\"blsk\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-bluesky\"></i>\r\n  </a>\r\n  <a class=\"facebook\" :href=\"facebook\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-facebook\"></i>\r\n  </a>\r\n        </div>\r\n      </div>\r\n        \r\n        \r\n        <div class=\"content\" v-html=\"content\"></div>\r\n        <div class=\"sns-wrap\">\r\n          <div class=\"sns_bottom\">\r\n          <p>SNSへのシェアはこちらから</p>\r\n        <div class=\"sns bottom\">\r\n          <a class=\"twitter\" :href=\"twitter\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-x-twitter\"></i>\r\n  </a>\r\n  <a class=\"line\" :href=\"line\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-line\"></i>\r\n  </a>\r\n  <a class=\"bluesky\" :href=\"blsk\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-bluesky\"></i>\r\n  </a>\r\n  <a class=\"facebook\" :href=\"facebook\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n    <i class=\"fa-brands fa-facebook\"></i>\r\n  </a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    </div>\r\n  </div>\r\n  </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { articles } from '@/utiles/articles'\r\n  import Meta from './meta.vue'\r\n\r\n\r\n  export default {\r\n    name: 'PostComponent',\r\n\r\n    components: {\r\n      Meta\r\n  },\r\n  emits: [ 'pageTitle' ],\r\n    data() {\r\n      return{\r\n        title : '',\r\n        content : '',\r\n        date : '',\r\n        thumb:'',\r\n        category:'',\r\n        tag:'',\r\n        link:'',\r\n        twitter:'',\r\n        line:'',\r\n        blsk:'',\r\n        facebook:'',\r\n        loading:true\r\n      };\r\n    },\r\n    created(){\r\n      this.loading=true\r\n    },\r\n    mounted() {\r\n      \r\n      this.postContent();\r\n      \r\n    },\r\n    updated(){\r\n      this.loading = false;\r\n    },\r\n    methods: {\r\n      buttonChange(){\r\n\r\n        const tech = document.querySelector('.fa-wrench')\r\n        const blog = document.querySelector('.fa-newspaper')\r\n\r\n        if(this.category === '技術系') {\r\n          if (tech.classList.contains('no-select')) {\r\n        tech.classList.remove('no-select')\r\n        blog.classList.add('no-select')\r\n      } else {\r\n        if(blog.classList.contains('no-select') === false)\r\n        blog.classList.add('no-select')\r\n      }\r\n        } else if (this.category === 'ブログ') {\r\n          if (blog.classList.contains('no-select')) {\r\n        blog.classList.remove('no-select')\r\n        tech.classList.add('no-select')\r\n      } else {\r\n        if(tech.classList.contains('no-select') === false)\r\n        tech.classList.add('no-select')\r\n      }\r\n        }\r\n      },\r\n      postContent(){\r\n        let ths = this;\r\n        let forApi = this.$route.path;\r\n        let idNumber = forApi.replace('/post','')\r\n        let id = Number(idNumber) + 1;\r\n\r\n\r\n          ths.title = articles[id].title,\r\n          ths.content = articles[id].content,\r\n          ths.date = articles[id].date,\r\n          ths.tag =  articles[id].tag,\r\n          ths.category = articles[id].category,\r\n          ths.link = 'http://localhost:8080/post' + articles[id].id\r\n          ths.twitter = 'https://x.com/intent/tweet?url=' + ths.link +'&text='+ ths.title + ' | ブログ'\r\n          ths.line = 'https://social-plugins.line.me/lineit/share?url=' + ths.link\r\n          ths.blsk = 'https://bsky.app/intent/compose?text='+ths.title + ' | ブログ'+ ths.link\r\n          ths.facebook = 'http://www.facebook.com/share.php?u='+ths.link\r\n          \r\n          ths.buttonChange();\r\n\r\n  \r\n      }\r\n    }\r\n  }\r\n  </script>\r\n  <style>\r\n  .wrap {\r\n    padding: 50px 0;\r\n  }\r\n\r\n  .wrap-inner {\r\n    \r\n    max-width: 600px;\r\n    margin: auto;\r\n    background-color: rgb(255, 255, 255);\r\n    \r\n\r\n  }\r\n\r\n  .article_inner {\r\n    margin: 0 25px;\r\n    \r\n  }\r\n\r\n  .title {\r\n    margin: 10px 0;\r\n    font-size: 30px;\r\n    font-weight: bold;\r\n    line-height: 1.3;\r\n  }\r\n\r\n  .cat_tag {\r\n    display: flex;\r\n    gap: 10px;\r\n  }\r\n\r\n\r\n\r\n  .cat_data 