{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ArticleSearch from \"../components/ArticleSearch.vue\";\nimport Paginate from \"vuejs-paginate-next\";\nimport Meta from '../components/meta.vue';\nimport axios from 'axios';\nexport default {\n  name: \"articleList\",\n  components: {\n    ArticleSearch,\n    Paginate,\n    Meta\n  },\n  inject: [\"blog\"],\n  data() {\n    return {\n      pageCount: 1,\n      showlists: [],\n      baselists: [],\n      lists: [],\n      pageRange: 4,\n      frc: 1,\n      loading: true\n      //force-pageが今のページネーション側のページ数の数字なので、絞り込みが発動した時にそこの値を1に変えちまえばページネーションの挙動がおかしくならないんでつね～！\n\n      //listとshowlistの間にもう一個配列が必要。絞り込み検索のときにlistから絞り込み条件に合致する内容を持ってきて配列を作って、そこからshowlistに持ってくる形にしないといけない。なのでページネーションの数を決めるやつも真ん中のヤツベースにする。真ん中のやつとページネーションでshowlistと真ん中のやつを更新するようにすればリアクティブ化は成立するかな？なのでコンポーネント間の値の受け渡しをできるようにこれを書き換えないといけないワケなんだよなぁ。\n    };\n  },\n  methods: {\n    buttonChange() {\n      const radio = document.querySelector('.fa-radio');\n      const blog = document.querySelector('.fa-newspaper');\n      if (blog.classList.contains('no-select')) {\n        blog.classList.remove('no-select');\n        radio.classList.add('no-select');\n      } else {\n        if (radio.classList.contains('no-select') === false) radio.classList.add('no-select');\n      }\n    },\n    clickCallback(pageNumber) {\n      this.frc = pageNumber;\n      this.showlists = this.baselists.slice(this.pageRange * (pageNumber - 1), this.pageRange * pageNumber);\n    },\n    catSea(e) {\n      if (e === \"\") {\n        this.baselists = this.lists;\n        this.pageCount = Math.ceil(this.baselists.length / this.pageRange);\n        this.showlists = this.baselists.slice(0, 4);\n        this.frc = 1;\n      } else {\n        this.baselists = [];\n        for (let i = 0; i < this.lists.length; i++) {\n          if (e === this.lists[i].tag) {\n            this.baselists.push(this.lists[i]);\n          }\n        }\n        this.pageCount = Math.ceil(this.baselists.length / this.pageRange);\n        this.showlists = this.baselists.slice(0, 4);\n        this.frc = 1;\n      }\n      //ページネーションを操作するなりなんなりの処理を入れて「絞り込み時に表示も切り替わる」ようにしなきゃいけないですねえ\n      //ページカウントも可変式に\n    },\n    postGive() {\n      let ths = this;\n      const radioArray = [];\n      const blogArray = [];\n      axios.get('https://pinky-hanger.com/wp/wp-json/wp/v2/posts/').then(function (response) {\n        for (let i = 0; i < response.data.length; i++) {\n          if (response.data[i].categories_info[0].name === \"動画\") {\n            let dates = new Date(response.data[i].date);\n            radioArray.push({\n              category: response.data[i].categories_info[0].name,\n              title: response.data[i].title.rendered,\n              date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\n              thumb: response.data[i].thumbnail_url,\n              tag: response.data[i].tags_info[0].name,\n              link: '/post' + response.data[i].id\n            });\n          } else if (response.data[i].categories_info[0].name === \"ブログ\") {\n            let dates = new Date(response.data[i].date);\n            blogArray.push({\n              category: response.data[i].categories_info[0].name,\n              title: response.data[i].title.rendered,\n              date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\n              thumb: response.data[i].thumbnail_url,\n              tag: response.data[i].tags_info[0].name,\n              link: '/post' + response.data[i].id\n            });\n          } else {\n            console.log(\"とばすよ～ん\");\n          }\n        }\n        ths.$emit('postTo', radioArray, blogArray);\n        ths.lists = blogArray;\n        ths.listin();\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    listin() {\n      this.baselists = this.lists;\n      this.showlists = this.baselists.slice(0, this.pageRange);\n      this.pageCount = Math.ceil(this.baselists.length / this.pageRange);\n    }\n  },\n  created() {\n    this.loading = true;\n  },\n  mounted() {\n    if (this.blog.length === 0) {\n      this.postGive();\n    } else {\n      this.lists = this.blog;\n      this.listin();\n    }\n    this.buttonChange();\n  },\n  updated() {\n    this.loading = false;\n  }\n};","map":{"version":3,"names":["ArticleSearch","Paginate","Meta","axios","name","components","inject","data","pageCount","showlists","baselists","lists","pageRange","frc","loading","methods","buttonChange","radio","document","querySelector","blog","classList","contains","remove","add","clickCallback","pageNumber","slice","catSea","e","Math","ceil","length","i","tag","push","postGive","ths","radioArray","blogArray","get","then","response","categories_info","dates","Date","date","category","title","rendered","getFullYear","getMonth","getDate","thumb","thumbnail_url","tags_info","link","id","console","log","$emit","listin","catch","error","created","mounted","updated"],"sources":["C:\\Users\\uhouh\\Documents\\pink-hunger\\pink-hunger\\src\\views\\ArticleList.vue"],"sourcesContent":["<template>\r\n  <Meta></Meta>\r\n  <div v-show=\"loading\" class=\"loader\">\r\n    <div class=\"loader-inner\">\r\n      <p>よみこみちゅうだよ～ん</p>\r\n    </div>\r\n  </div>\r\n  <div v-show=\"!loading\" class=\"out\">\r\n    <article-search v-on:category-search=\"catSea\"></article-search>\r\n    <div class=\"article_list\">\r\n      <div\r\n        class=\"article_cards\"\r\n        v-for=\"item in showlists\"\r\n        v-bind:key=\"item.link\"\r\n      >\r\n        <div class=\"article_thumb\">\r\n          <router-link class=\"thumb_link\" v-bind:to=\"item.link\">\r\n            <img :src=\"item.thumb\" />\r\n          </router-link>\r\n        </div>\r\n        <div class=\"article_txt\">\r\n          <div class=\"cat_day\">\r\n            <div class=\"category\">{{ item.tag }}</div>\r\n            <div class=\"day\">{{ item.date }}</div>\r\n          </div>\r\n          <div class=\"title\">\r\n            <router-link v-bind:to=\"item.link\">\r\n              <h1>{{ item.title }}</h1>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <paginate\r\n      :page-count=\"pageCount\"\r\n      :click-handler=\"clickCallback\"\r\n      :page-range=\"pageRange\"\r\n      :prev-text=\"'<'\"\r\n      :next-text=\"'>'\"\r\n      :force-page=\"frc\"\r\n    ></paginate>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ArticleSearch from \"../components/ArticleSearch.vue\";\r\nimport Paginate from \"vuejs-paginate-next\";\r\nimport Meta from '../components/meta.vue'\r\nimport axios from 'axios'\r\n\r\n\r\nexport default {\r\n  name: \"articleList\",\r\n  components: { ArticleSearch, Paginate,Meta },\r\n  inject: [\"blog\"],\r\n  data() {\r\n    return {\r\n      pageCount: 1,\r\n      showlists: [],\r\n      baselists: [],\r\n      lists: [],\r\n      pageRange: 4,\r\n      frc: 1,\r\n      loading:true\r\n      //force-pageが今のページネーション側のページ数の数字なので、絞り込みが発動した時にそこの値を1に変えちまえばページネーションの挙動がおかしくならないんでつね～！\r\n\r\n      //listとshowlistの間にもう一個配列が必要。絞り込み検索のときにlistから絞り込み条件に合致する内容を持ってきて配列を作って、そこからshowlistに持ってくる形にしないといけない。なのでページネーションの数を決めるやつも真ん中のヤツベースにする。真ん中のやつとページネーションでshowlistと真ん中のやつを更新するようにすればリアクティブ化は成立するかな？なのでコンポーネント間の値の受け渡しをできるようにこれを書き換えないといけないワケなんだよなぁ。\r\n    };\r\n  },\r\n  methods: {\r\n    buttonChange(){\r\n      const radio = document.querySelector('.fa-radio')\r\n      const blog = document.querySelector('.fa-newspaper')\r\n      if (blog.classList.contains('no-select')) {\r\n        blog.classList.remove('no-select')\r\n        radio.classList.add('no-select')\r\n      } else {\r\n        if(radio.classList.contains('no-select') === false)\r\n        radio.classList.add('no-select')\r\n      }\r\n    },\r\n    clickCallback(pageNumber) {\r\n      this.frc = pageNumber;\r\n      this.showlists = this.baselists.slice(\r\n        this.pageRange * (pageNumber - 1),\r\n        this.pageRange * pageNumber\r\n      );\r\n    },\r\n    catSea(e) {\r\n      if (e === \"\") {\r\n        this.baselists = this.lists;\r\n        this.pageCount = Math.ceil(this.baselists.length / this.pageRange);\r\n        this.showlists = this.baselists.slice(0, 4);\r\n        this.frc = 1;\r\n      } else {\r\n        this.baselists = [];\r\n        for (let i = 0; i < this.lists.length; i++) {\r\n          if (e === this.lists[i].tag) {\r\n            this.baselists.push(this.lists[i]);\r\n          }\r\n        }\r\n        this.pageCount = Math.ceil(this.baselists.length / this.pageRange);\r\n        this.showlists = this.baselists.slice(0, 4);\r\n        this.frc = 1;\r\n      }\r\n      //ページネーションを操作するなりなんなりの処理を入れて「絞り込み時に表示も切り替わる」ようにしなきゃいけないですねえ\r\n      //ページカウントも可変式に\r\n    },\r\n    postGive(){\r\n      let ths = this;\r\n      const radioArray = []\r\n      const blogArray = []\r\n    axios\r\n    .get('https://pinky-hanger.com/wp/wp-json/wp/v2/posts/')\r\n    .then(function(response){\r\n\r\n      for(let i = 0; i < response.data.length; i++) {\r\n        if(response.data[i].categories_info[0].name === \"動画\"){\r\n          let dates = new Date(response.data[i].date)\r\n          radioArray.push(\r\n            { \r\n              \r\n              category: response.data[i].categories_info[0].name,\r\n              title: response.data[i].title.rendered,\r\n              date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\r\n              thumb: response.data[i].thumbnail_url,\r\n              tag:  response.data[i].tags_info[0].name,\r\n              link : '/post' + response.data[i].id,\r\n            }\r\n            \r\n          )\r\n\r\n\r\n      } else if(response.data[i].categories_info[0].name === \"ブログ\"){\r\n        let dates = new Date(response.data[i].date)\r\n        blogArray.push(\r\n          { \r\n            \r\n            category: response.data[i].categories_info[0].name,\r\n            title: response.data[i].title.rendered,\r\n            date: dates.getFullYear() + '-' + (dates.getMonth() + 1) + '-' + dates.getDate(),\r\n            thumb: response.data[i].thumbnail_url,\r\n            tag:  response.data[i].tags_info[0].name,\r\n            link : '/post' + response.data[i].id,\r\n          }\r\n          \r\n        )\r\n\r\n\r\n    } else {\r\n          console.log(\"とばすよ～ん\")\r\n        }\r\n      }\r\n      ths.$emit('postTo',radioArray,blogArray)\r\n      ths.lists = blogArray\r\n \r\n      ths.listin()\r\n    })\r\n    .catch(function(error) {\r\n      console.log(error);\r\n    });\r\n    },\r\n    listin(){\r\n           this.baselists = this.lists\r\n           this.showlists = this.baselists.slice(0,this.pageRange)\r\n           this.pageCount = Math.ceil(this.baselists.length/this.pageRange);\r\n    }\r\n  },\r\n  created(){\r\n    this.loading=true\r\n  },\r\nmounted() {\r\n  if(this.blog.length===0){\r\n  \r\n    this.postGive()\r\n    \r\n  } else {\r\n    this.lists = this.blog\r\n    this.listin();\r\n  }\r\n  \r\n  \r\n  this.buttonChange();\r\n    }\r\n    ,updated(){\r\n      this.loading = false;\r\n    }\r\n};\r\n</script>\r\n\r\n<style>\r\n.loader {\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.out {\r\n  padding: 50px 0;\r\n  min-height: calc(100vh);\r\n  background: #f1f0ed;\r\n}\r\n\r\n.article_list {\r\n  display: grid;\r\n  grid-template-columns: 1fr; /* 子要素を縦並び */\r\n  gap: 0; /* 隙間なし */\r\n  max-width: 800px;\r\n  margin: 0 auto; /* 中央寄せ */\r\n}\r\n\r\n.article_cards {\r\n  display: flex;\r\n  align-items: flex-start;\r\n  padding: 16px;\r\n  position: relative;\r\n}\r\n\r\n.article_cards::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  border-bottom: 2px dashed #ccc; /* 破線の区切り */\r\n}\r\n\r\n.article_thumb {\r\n  display: flex;\r\n  align-items: center;\r\n  flex: 0 0 30%;\r\n  max-width: 30%;\r\n  min-wid