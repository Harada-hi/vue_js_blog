og)\n    }\n  },\n  methods: {\n    postList(){\n      let ths = this;\n      axios\n      .get('http://localhost/radio-site/wordpress/wp-json/wp/v2/posts/')\n      .then(function(response){\n        console.log(response.data.length)\n        for(let i = 0; i < response.data.length; i++) {\n          if(response.data[i].categories_info[0].name === \"ブログ\") {\n            ths.blog.push(\n              {\n                category: response.data[i].categories_info[0].name,\n                title: response.data[i].title.rendered,\n                date: response.data[i].date,\n                thumb: response.data[i].thumbnail_url,\n                tag:  response.data[i].tags_info[0].name\n              }\n            ) \n          } else if(response.data[i].categories_info[0].name === \"ラジオ\"){\n            ths.radio.push(\n              {\n                category: response.data[i].categories_info[0].name,\n                title: response.data[i].title.rendered,\n                date: response.data[i].date,\n                thumb: response.data[i].thumbnail_url,\n                tag:  response.data[i].tags_info[0].name\n              }\n            ) \n          } else {\n            console.log(\"error\")\n          }\n        }\n      ths.loading = false\n      })\n      .catch(function(error) {\n        console.log(error);\n      });\n\n    }\n  }\n    \n  \n}\n</script>\n\n<style>\n@import \"./css/destyle.css\";\n#app {\n  background-color: rgb(255, 245, 220);\n}\n\n</style>\n"],"mappings":";;EAEwBA,KAAK,EAAC;AAAQ;;;;;uBAFtCC,mBAAA,CAAAC,SAAA,SACEC,YAAA,CAAiBC,iBAAA,G,gBACjBC,mBAAA,CAAyD,OAAzDC,UAAyD,EAApB,gBAAc,0B,SAAtCC,KAAA,CAAAC,OAAO,E,mBACpBH,mBAAA,CAEI,cADJF,YAAA,CAAcM,sBAAA,E,oCADAF,KAAA,CAAAC,OAAO,E,GAGrBL,YAAA,CAAiBO,iBAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}