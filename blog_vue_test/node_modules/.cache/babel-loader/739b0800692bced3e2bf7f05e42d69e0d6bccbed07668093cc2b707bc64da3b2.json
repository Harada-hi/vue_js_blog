{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Footer from './components/Footer.vue';\nimport Header from './components/Header.vue';\nimport axios from 'axios';\nexport default {\n  name: 'app',\n  components: {\n    Footer,\n    Header\n  },\n  created: function () {\n    this.postList();\n  },\n  data() {\n    return {\n      radio: [],\n      blog: [],\n      loading: true\n    };\n  },\n  provide() {\n    return {\n      radio: Vue.computed(() => this.radio),\n      blog: Vue.computed(() => this.blog)\n    };\n  },\n  methods: {\n    postList() {\n      let ths = this;\n      axios.get('http://localhost/radio-site/wordpress/wp-json/wp/v2/posts/').then(function (response) {\n        console.log(response.data.length);\n        for (let i = 0; i < response.data.length; i++) {\n          if (response.data[i].categories_info[0].name === \"ブログ\") {\n            ths.blog.push({\n              category: response.data[i].categories_info[0].name,\n              title: response.data[i].title.rendered,\n              date: response.data[i].date,\n              thumb: response.data[i].thumbnail_url,\n              tag: response.data[i].tags_info[0].name\n            });\n          } else if (response.data[i].categories_info[0].name === \"ラジオ\") {\n            ths.radio.push({\n              category: response.data[i].categories_info[0].name,\n              title: response.data[i].title.rendered,\n              date: response.data[i].date,\n              thumb: response.data[i].thumbnail_url,\n              tag: response.data[i].tags_info[0].name\n            });\n          } else {\n            console.log(\"error\");\n          }\n        }\n        ths.loading = false;\n      }).catch(function (error) {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Footer","Header","axios","name","components","created","postList","data","radio","blog","loading","provide","Vue","computed","methods","ths","get","then","respons